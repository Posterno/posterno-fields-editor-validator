{
	"name": "posterno/posterno-fields-editor-validator",
	"description": "Posterno component that provides validation of settings for Carbon fields used within the fields editor.",
	"keywords": [
		"wordpress"
	],
	"homepage": "https://posterno.com",
	"authors": [{
		"name": "Posterno",
		"email": "hello@posterno.com"
	}],
	"require": {
		"php": ">=5.6",
		"composer/installers": "~1.0"
	},
	"autoload": {
		"files" : [
			"scripts-loader.php"
		]
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "0.4.4",
		"pressmodo/phpcs-config": "dev-master"
	},
	"scripts": {
		"lint": "phpcs .",
		"lint-fix": "phpcbf ."
	}
}
